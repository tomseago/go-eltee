{
      "source": "./src"
    , "destination": "./docs"
    , "excludes": [
        "^\\."
    ]
    , "plugins": [
          { "name": "esdoc-standard-plugin" }
        , { "name": "esdoc-jsx-plugin" }
        , { 
            "name": "esdoc-ecmascript-proposal-plugin"
            , "option": {
                "classProperties": true,
                "objectRestSpread": false,
                "doExpressions": false,
                "functionBind": false,
                "functionSent": false,
                "asyncGenerators": false,
                "decorators": false,
                "exportExtensions": false,
                "dynamicImport": false
            }
        }
    ]
    , "off": [
        { "name": "esdoc-jsx-plugin" }
        , { "name": "esdoc-ecmascript-proposal-plugin" }
    ]
}